# ignore logs
log/*.log

# ignore db schema and seeds
db/schema.rb
db/seeds.rb

# ignore notes
NOTES/

# ignore yml
config/*.yml
!config/cucumber.yml

# ignore config files containing secret keys, sensitive info, etc. (TO DO: move these to yml files)
config/environment.rb
config/initializers/session_store.rb
config/initializers/site_keys.rb
config/initializers/geokit_config.rb

# ignore vendor, keep git repos slim...
vendor/

# ignore tmp and autogenerated files
tmp/**/*
tmp/*.*
rerun.txt

# ignore data files
script/import/data/*.*
!script/import/data/*.rb

# ignore some public content
public/.htaccess.mod_gzip.bak
public/photos/
public/avatars/

# ignore backups
backups/

# ignore some docs
doc/*.txt
doc/app/

# ignore sass cache
.sass-cache/

# ignore other stuff
.DS_Store
.svn/
*.tmproj
